# DeepSeek API Configuration
# Copy this file to .env and replace with your actual API key
DEEPSEEK_API_KEY=your_deepseek_api_key_here
DEEPSEEK_MODEL=deepseek-chat
DEEPSEEK_BASE_URL=https://api.deepseek.com/v1

# Baidu Speech API Configuration
# Get your Baidu Speech API credentials from: https://ai.baidu.com/tech/speech
BAIDU_APP_ID=your_baidu_app_id_here
BAIDU_API_KEY=your_baidu_api_key_here
BAIDU_SECRET_KEY=your_baidu_secret_key_here

# App Configuration
APP_NAME=SugarAssistant
APP_VERSION=1.0.0 